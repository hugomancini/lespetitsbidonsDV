  .hvr-border-fade a {
    height: 43px !important;
    padding-top: 9px !important;
    display: inline-block;
    vertical-align: middle;
    -webkit-transform: perspective(1px) translateZ(0);
    transform: perspective(1px) translateZ(0);
    box-shadow: 0 0 1px rgba(0, 0, 0, 0);
    -webkit-transition-duration: 0.3s;
    transition-duration: 0.3s;
    -webkit-transition-property: box-shadow;
    transition-property: box-shadow;
    box-shadow: inset 0 0 0 0px #e1e1e1, 0 0 0px rgba(0, 0, 0, 0);
    padding: 0 15px;
    /* Hack to improve aliasing on mobile/tablet devices */
  }
  .hvr-border-underline a {
    padding-bottom: 10px !important;
      margin-top: 10px !important;
    display: inline-block;
    vertical-align: middle;
    -webkit-transform: perspective(1px) translateZ(0);
    transform: perspective(1px) translateZ(0);
    box-shadow: 0 0 1px rgba(0, 0, 0, 0);
    -webkit-transition-duration: 0.3s;
    transition-duration: 0.3s;
    -webkit-transition-property: box-shadow;
    transition-property: box-shadow;
    box-shadow: inset 0 0 0 0px #e1e1e1, 0 0 0px rgba(0, 0, 0, 0);
    padding: 0 15px;
    /* Hack to improve aliasing on mobile/tablet devices */
  }
  .hvr-border-fade a:hover,
  .hvr-border-fade a:focus,
  .hvr-border-fade a:active {

    box-shadow: inset 0 0 0 4px $main_color, 0 0 1px rgba(0, 0, 0, 0) !important;
    border: none !important;
    /* Hack to improve aliasing on mobile/tablet devices */
  }
  .hvr-border-underline a:hover,
  .hvr-border-underline a:focus,
  .hvr-border-underline a:active {
    box-shadow: inset 2px -7px 0px -4px $main_color, 2px 1px 0px -2px  rgba(0,0,0,0) !important;
    border: none !important;
