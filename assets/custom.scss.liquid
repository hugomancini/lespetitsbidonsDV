      #progressbar {
        height: 170px;
      }
      #progressbar,
      #progressbar-text {
          margin: 10px 0 30px 0;
          padding: 0;
          overflow: hidden;
          display: block;
      }

      #progressbar li,
      #progressbar-text li {
          font-family: 'Roboto', Helvetica;
          list-style-type: none;
          color: $main_color;
          text-transform: uppercase;
          font-size: 12px;
          width: 20%;
          height: 100%;
          float: left;
          position: relative;
          font-weight: 500;
          display: flex;
          flex-direction: column-reverse;
          img {
            top: 50%;
            position: absolute;
            left: 50%;
            transform: translate(-50%,-50%);
            opacity: 0.4;
            width: 65%;
          }
      }



      #progressbar li:before {
          width: 100%;
          height: 2px;
          font-family: 'Roboto';
          font-size: 25px;
          line-height: 17px;
          border: 1px solid lightgrey;
          background-color: lightgrey;
          display: block;
          margin: 2px auto 24px auto;
      }

      #progressbar li.is-nav-selected{
        img {
          opacity: 1;
          transition: 1s;
        }
          &:before {
          border: 0px solid blue;
          width: 100%;
          height: 6px;
          font-family: gorditabold;
          font-size: 25px;
          line-height: 20px;
          background-color: $main_color;
          // background-image: url("../images/select.png");
          background-size: 20px;
          position: absolute;
          bottom:-2px;
          // background: $blue-slider;
        }
      }
      :after, :before {
          -webkit-box-sizing: border-box;
          -moz-box-sizing: border-box;
          box-sizing: border-box;
      }

      #progressbar-text li:not(:first-child):after {
        content: ">";
        width: 100%;
        top: 0px;
        width: 3px;
        height: 5px;
        z-index: 3;
        left: 0px;
      }
      #progressbar li:first-child:after {
          content: none;
      }
      #progressbar li:before {
          content: "";
      }
    }
    // TIMELINE


  // Scroll MAgic

  @keyframes parag {
    from {
      opacity: 0
    }


    to {
      opacity: 1
    }
  }


  .scroll-magic {
    .animation {
      max-height: 100vh;
      position: relative;

      .logo-line {
        .logo-active {
          position: absolute;
          opacity: 0;
          transition: 1.5s !important;
        }
        .logo-active.active {
          opacity: 1;
          display: block;
          transition: 1.5s !important;
        }



        position: absolute;
        height: calc(30vw + 3vw);
        left: calc(5vw - 1.5vw);
        top: calc(5vw - 1.5vw);
        display: flex;
        flex-direction: column;
        justify-content: space-between;
        z-index: 1;
        .step {
          display: flex;
          transition: 1.5s height;

          justify-content: space-between;

          .logo-label {
            width: 15vw;
            align-self: center;
            padding: 0 10px 0 0;
            margin: 0;
            text-align: right;
            font-family: gorditamedium;
            font-size: 15px;
          }
          img {
            align-self: center;
            width: 3vw;
            height: 3vw;
            min-width: 3vw;
            max-width: 3vw;
            min-height: 3vw;
            max-height: 3vw;

          }


        }
        div.after {
            position: absolute !important;}
        .step:not(:last-child).active {
          // transition: 1.5s height;
          position: relative;

          div.after {
            position: absolute !important;
            top: 3vw;
            right: calc(1.5vw - 2px);
            height: 50px;
            // transition: 1.5s height;
            border: 2px solid $main_color;
            height: 5px;
            z-index: -1;

          }
        }
      }

      .animated {
        transition: 1s;
        opacity: 0;
        position: absolute;
        display: flex;
        justify-content: space-between;
        #biglogo1 {
          display:none;
        }
        .big-logo {
            left: 80vw;
            top: -98px;
            position: absolute;
            animation-duration: 1.5s;
            animation-name: parag;


        }
        .left {
          position: absolute;
          top:5vw;
          left:20vw;
          width: 30vw;
          height: 30vw;
          // z-index: -1;
          .image-element__wrap {
            // z-index: -1;
            margin-left: 0 !important;
            margin-right: 0 !important;
          }

          img {
            width: 30vw;

            // z-index: -1;

            height: 30vw;
            object-fit: cover;

          }
        }
        .right {
          position: absolute;
          top:5vw;
          left:55vw;
          width: 30vw;
          height: 35vw;
          height: max-content !important;
          .headline {
            text-align: left !important;
            padding: 0 !important;
            margin-bottom: 0 !important;

          }
          .subtitle {
            font-size: 1.0625rem !important;


          }
          .content {

            padding: 0px 0px 15px 0 !important;

          }


        }
      }
      .animated.active, #high1 {
        opacity:1;
        transition: 1s;
        .content {
          animation-duration: 0.5s;
          animation-name: parag;
        }

      }
    }

    .logos-mob {
      display: flex;
      flex-wrap: wrap;
      justify-content: center;
      .logo-mob {
        &.selected {
          background: {{settings.btn_white_hover_color}}
        }
        max-width: 50%;
        flex: 1 0 50%;
        padding: 15px;
        img {
          width: 40%;
          margin-left: 30%;
        }
        .logo-mob-label {
          text-align: center;
          font-family: 'gorditabold'
        }
      }


    }
    .content_for_logo {
      padding: 15px;
      .subbutton2 {
        text-align: center !important;
      }
      .content_text {
        display: none;
      }
    }



  }



